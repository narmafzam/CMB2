{
    "name": "narmafzam/cmb2",
    "description": "CMB2 is a metabox, custom fields, and forms library for WP that will blow your mind.",
    "license": "proprietary",
    "keywords": ["wordpress", "plugin", "metabox"],
    "homepage": "https://github.com/narmafzam/CMB2",
    "type": "project",
    "require": {
        "php": ">5.2.4"
    },
    "suggest": {
        "composer/installers": "~1.0"
    },
    "require-dev": {
        "apigen/apigen": "4.1.2",
        "nette/utils": "2.5.3",
        "phpunit/phpunit": "6.5.13"
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ],
        "docs": [
            "vendor/bin/apigen generate --config apigen/apigen.neon",
            "php apigen/hook-docs.php"
        ]
    }
}
